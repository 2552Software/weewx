    <td class="label">Latitude:</td>
    <td><span class="data">$station.latitude[0]&deg; $station.latitude[1]' $station.latitude[2]</span></td>
  </tr>
  <tr>
    <td class="label">Longitude:</td>
    <td><span class="data">$station.longitude[0]&deg; $station.longitude[1]' $station.longitude[2]</span></td>
  </tr>
  <tr>
    <td class="label">Elevation:</td>
    <td><span class="data">$station.altitude</span></td>
  </tr>
#if $almanac.hasExtras
  #if $almanac.sun.set.raw is not None and $almanac.sun.rise.raw is not None
    #set $daylight_s = $almanac.sun.set.raw - $almanac.sun.rise.raw
    #set $daylight_h_str = "%02d" % int($daylight_s / 3600)
    #set $daylight_m_str = "%02d" % int(($daylight_s % 3600) / 60)
  #else
    #set $daylight_h_str = "00"
    #set $daylight_m_str = "00"
  #end if
  <tr>
    <td class="label">Daylight:</td>
    <td><span class="data">$daylight_h_str:$daylight_m_str</span></td>
  </tr>
#end if
</table>

    </td>
    <td>&nbsp;</td>
    <td>

