#errorCatcher Echo
#encoding UTF-8
#set global $page_title = $Extras.labels.archive
#set global $btn = {'archive':'button_active'}
<!DOCTYPE html>
  <head>
    <title>$station.location : $page_title</title>
    <link rel="stylesheet" type="text/css" href="weewx.css"/>
    <link rel="icon" type="image/png" href="favicon.ico" />
    #include "ga.inc"
    <style>
      .selectors {
        clear: right;
        float: right;
        text-align: right;
      }
    </style>
    <script type="text/javascript">
      function openURL(urlname) {
        window.location = urlname;
      }
      function openNoaaFile(date) {
        window.location = "NOAA/NOAA-" + date + ".txt";
      }
    </script>
  </head>
  <body>

    #include "header.inc"

    <div class='selectors'>
      <p>Monthly Summary:&nbsp;
        <select NAME="noaaselect" onchange="openNoaaFile(value)">
          #for $monthYear in $SummaryByMonth
          <option value="$monthYear">$monthYear</option>
          #end for
	  <option selected>-Select month-</option>
        </select>
        <br/>
        Yearly Summary:&nbsp;
        <select NAME="noaaselect" onchange="openNoaaFile(value)">
          #for $y in $SummaryByYear
          <option value="$y">$y</option>
          #end for
	  <option selected>-Select year-</option>
        </select>
      </p>
    </div>

    <div id='content'>

<table class='metrics'>
  <tr style='vertical-align:top'>
    <td>

<table>
  <tr>
    <td class="label">Station:</td>
    <td><span class="data">$station.hardware</span></td>
  </tr>
  <tr>
    <td class="label">Time:</td>
    <td><span class="data">$current.dateTime.format("%H:%M")</span></td>
  </tr>
  <tr>
    <td class="label">Date:</td>
    <td><span class="data">$current.dateTime.format("%d %b %Y")</span></td>
  </tr>
